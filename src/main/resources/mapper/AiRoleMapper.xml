<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.work.plat.mapper.AiRoleMapper">
    <sql id="tableName">
        ai_role
    </sql>

    <sql id="baseColumn">
        id,title,desc,params,type,create_time
    </sql>
    <select id="getRoleKey" resultType="java.lang.String">
        select role_key from ai_role where type = #{type}
    </select>

</mapper>
